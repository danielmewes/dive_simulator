{
  "name": "scuba-decompression-simulator",
  "version": "1.0.0",
  "description": "An interactive browser-based simulator for observing and comparing different scuba diving decompression models in real-time",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "build:browser": "webpack --mode=production",
    "build:dev": "webpack --mode=development",
    "serve": "webpack serve --mode=development",
    "test": "jest",
    "test:watch": "jest --watch",
    "dev": "tsc --watch",
    "lint": "eslint src/**/*.ts",
    "start": "node dist/test-basic.js",
    "demo": "npx ts-node src/run-demo.ts",
    "test:basic": "npx ts-node src/test-basic.ts"
  },
  "keywords": [
    "scuba",
    "diving",
    "decompression",
    "vpm-b",
    "bvm-3",
    "bubble-model",
    "buhlmann",
    "simulator",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "html-webpack-plugin": "^5.6.3",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.0",
    "typescript": "^5.0.0",
    "webpack": "^5.99.9",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.2"
  }
}
